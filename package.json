{
  "name": "react-bootstrap-4",
  "version": "0.29.1",
  "description": "Temporary fork of react-bootstrap for Bootstrap 4",
  "repository": {
    "type": "git",
    "url": "vatesfr/react-bootstrap"
  },
  "homepage": "http://react-bootstrap.github.io/",
  "scripts": {
    "build": "babel-node tools/build-cli.js",
    "test-watch": "karma start",
    "test-coverage": "COVERAGE=true karma start --single-run",
    "test": "npm run lint && npm run tests-set && npm run build",
    "tests-set": "karma start --single-run && _mocha --compilers js:babel-core/register test/server/*Spec.js",
    "lint": "eslint ./",
    "release": "release",
    "docs-build": "babel-node tools/build-cli.js --docs-only",
    "docs": "babel-node docs/dev-run",
    "docs-prod": "npm run docs-build && NODE_ENV=production babel-node docs/server.js",
    "docs-prod-unoptimized": "npm run docs-build -- --dev && NODE_ENV=production babel-node docs/server.js"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "CHANGELOG.md",
    "lib",
    "dist"
  ],
  "keywords": [
    "react",
    "ecosystem-react",
    "react-component",
    "bootstrap"
  ],
  "author": "Stephen J. Collings <stevoland@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-dev-expression": "^0.2.1",
    "bootstrap": "^3.3.5",
    "brfs": "^1.4.1",
    "chai": "^3.3.0",
    "child-process-promise": "^2.2.0",
    "codemirror": "^5.7.0",
    "colors": "^1.1.2",
    "css-loader": "^0.26.0",
    "es5-shim": "^4.1.14",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "fs-promise": "^1.0.0",
    "glob": "^7.1.1",
    "history": "^4.4.1",
    "http-proxy": "^1.11.2",
    "ip": "^1.0.1",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.3",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "~1.0.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lolex": "^1.4.0",
    "marked": "^0.3.5",
    "mocha": "^3.2.0",
    "node-libs-browser": "^2.0.0",
    "nodemon": "^1.7.1",
    "output-file-sync": "^1.1.1",
    "phantomjs": "^2.1.7",
    "portfinder": "^1.0.10",
    "react": "^15.4.1",
    "react-component-metadata": "^3.1.0",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^3.0.0",
    "react-waypoint": "^4.1.0",
    "release-script": "^1.0.2",
    "rimraf": "^2.4.3",
    "semver": "^5.0.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "yargs": "^6.4.0"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "classnames": "^2.1.5",
    "dom-helpers": "^3.0.0",
    "invariant": "^2.1.2",
    "keycode": "^2.1.0",
    "lodash-compat": "^3.10.1",
    "react-overlays": "^0.6.0",
    "react-prop-types": "^0.4.0",
    "uncontrollable": "^4.0.3",
    "warning": "^3.0.0"
  },
  "release-script": {
    "skipBuildStep": "true",
    "bowerRepo": "git@github.com:react-bootstrap/react-bootstrap-bower.git",
    "docsRepo": "git@github.com:react-bootstrap/react-bootstrap.github.io.git"
  }
}
